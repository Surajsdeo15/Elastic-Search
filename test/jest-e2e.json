{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/elastic": "<rootDir>/../libs/elastic/src",
    "@app/elastic-search": "<rootDir>/../libs/elastic-search/src",
    "@app/elastic-search/(.*)": "<rootDir>/../libs/elastic-search/src/$1",
    "@app/elastic/(.*)": "<rootDir>/../libs/elastic/src/$1",
    "@app/elasticstack": "<rootDir>/../libs/elasticstack/src",
    "@app/elasticstack/(.*)": "<rootDir>/../libs/elasticstack/src/$1"
  }
}